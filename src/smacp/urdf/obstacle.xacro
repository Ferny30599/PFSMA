<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="obstacle">

  <link name="obs1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="1 1 1" />
      </geometry>
      <material name="yellow">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>

  <joint name="cube" type="fixed">
    <parent link="world"/>
    <child link="obs1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <gazebo reference="obs1">
 	<material>Gazebo/Red</material>
  </gazebo>

</robot>